{
  "name": "@openint/sdk",
  "version": "0.0.11",
  "private": false,
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "clean": "rm -rf ./dist",
    "gen": "pnpm gen:schema && pnpm gen:types",
    "gen:schema": "NEXT_PUBLIC_SERVER_URL=https://app.openint.dev npx tsx ../../apps/web/lib-server/appRouter.ts > ./openint.oas.json",
    "gen:types": "openapi-typescript ./openint.oas.json --output ./openint.oas.d.ts",
    "pub": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@openint/openapi-client": "workspace:*",
    "openapi3-ts": "4.1.2",
    "tsx": "3.12.2"
  },
  "devDependencies": {
    "openapi-typescript": "7.0.0-next.3"
  }
}
