{
  "name": "@ledger-sync/integration-yodlee",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "register.node.ts"
  ],
  "module": "./index.ts",
  "scripts": {
    "codegen": "openapi --name YodleeAPI -o ./yodlee.generated -i https://raw.githubusercontent.com/Yodlee/OpenAPI/main/swagger.yaml -c axios --useOptions --useUnionTypes --request ./request.noop.ts --exportCore true --exportSchemas true --indent 2",
    "codegen:next": "openapi-typescript https://raw.githubusercontent.com/Yodlee/OpenAPI/main/swagger.yaml --output yodlee.generated.ts --make-paths-enum"
  },
  "dependencies": {
    "@ledger-sync/cdk-core": "*",
    "@ledger-sync/cdk-ledger": "*",
    "@ledger-sync/standard": "*",
    "@ledger-sync/util": "*",
    "@react-hookz/web": "15.1.0",
    "csstype": "*",
    "framer-motion": "6.3.11",
    "react-dom": "*",
    "react-hot-toast": "2.3.0"
  },
  "devDependencies": {
    "@cfworker/json-schema": "1.12.4",
    "@types/react": "*",
    "openapi-typescript": "5.4.1",
    "openapi-typescript-codegen": "0.23.0",
    "react": "*"
  },
  "peerDependencies": {
    "react": "*"
  }
}
