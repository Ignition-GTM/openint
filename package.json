{
  "name": "alka-monorepo",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.com:alkafinance/ledger-sync.git",
  "license": "UNLICENSED",
  "workspaces": [
    "packages/@dev/*",
    "packages/@integrations/*",
    "packages/@utils/*",
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "yarn --cwd apps/next dev",
    "lint": "eslint --ext .js,.ts,.tsx --cache .",
    "mitm": "mitmweb --listen-port 3010 --web-port 3011 --mode reverse:http://localhost:3000",
    "postinstall": "{ patch-package || true; } && { husky install || true; }",
    "test::ci": "yarn run test --ci --forceExit --detectOpenHandles --runInBand",
    "test::debug": "node --inspect-brk $(npm bin)/jest --runInBand --watch",
    "test::watch": "yarn run test --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "typecheck": "tsc --project . --noEmit",
    "validate": "run-s --silent typecheck lint test",
    "ledgerSync": "tsx ./bin/ledgerSync"
  },
  "prettier": "./config/prettier.config.js",
  "eslintConfig": {
    "extends": "@ledger-sync/eslint-config"
  },
  "jest": {
    "preset": "./config/jest.config.js"
  },
  "resolutions": {
    "@trivago/prettier-plugin-sort-imports/**/@babel/parser": "7.18.9",
    "@types/node": "16.11.12",
    "axios": "0.21.1",
    "plaid": "10.7.0",
    "stripe": "8.86.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-transform-modules-commonjs": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@babel/types": "7.18.7",
    "@ledger-sync/eslint-config": "*",
    "@roarr/cli": "5.4.0",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/jest": "27.5.1",
    "@types/node": "16.11.12",
    "@types/prettier": "2.6.3",
    "esbuild": "0.14.42",
    "esbuild-jest": "0.5.0",
    "eslint": "8.16.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-date-mock": "1.0.8",
    "jest-watch-typeahead": "1.1.0",
    "lint-staged": "12.4.2",
    "npm-run-all": "4.1.5",
    "patch-package": "6.4.7",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.2.18",
    "tsx": "3.4.3",
    "typescript": "4.7.2"
  },
  "engines": {
    "node": "16",
    "npm": "7"
  }
}
