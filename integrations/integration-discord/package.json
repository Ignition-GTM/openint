{
  "name": "@usevenice/integration-discord",
  "version": "0.0.0",
  "private": true,
  "module": "./index.ts",
  "scripts": {
    "download": "curl https://raw.githubusercontent.com/discord/discord-api-spec/main/specs/openapi.json | json2yaml > ./discord.openapi.gen.yaml",
    "generate": "openapi-typescript ./discord.openapi.gen.yaml --output ./discord.openapi.gen.d.ts"
  },
  "dependencies": {
    "@usevenice/cdk": "workspace:*",
    "@usevenice/util": "workspace:*"
  },
  "devDependencies": {
    "json2yaml": "1.1.0",
    "openapi-typescript": "6.7.1"
  }
}
