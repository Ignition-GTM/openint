{
  "name": "@usevenice/integration-plaid",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "register.node.ts"
  ],
  "module": "./index.ts",
  "scripts": {
    "download:oas": "curl https://raw.githubusercontent.com/plaid/plaid-openapi/master/2020-09-14.yml > ./plaid.oas.yaml",
    "generate": "pnpm generate:oas && pnpm generate:types",
    "generate:oas": "pnpm download:oas && yaml2json ./plaid.oas.yaml | prettier --parser json > ./plaid.oas.json",
    "generate:types": "openapi-typescript ./plaid.oas.json --output plaid.oas.d.ts"
  },
  "dependencies": {
    "@t3-oss/env-core": "0.7.1",
    "@usevenice/cdk": "workspace:*",
    "@usevenice/util": "workspace:*",
    "plaid": "12.3.0",
    "react-plaid-link": "3.3.2"
  },
  "devDependencies": {
    "@types/react": "*",
    "openapi-typescript": "6.7.1",
    "prettier": "2.8.8",
    "yamljs": "0.3.0"
  },
  "peerDependencies": {
    "react": "*"
  }
}
